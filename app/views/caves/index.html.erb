<h1>Listing caves</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Lm</th>
      <th>Lp</th>
      <th>Mm</th>
      <th>Mp</th>
      <th>Mem</th>
      <th>Mep</th>
      <th>Gm</th>
      <th>Gp</th>
      <th>Vm</th>
      <th>Vp</th>
      <th>Sm</th>
      <th>Sp</th>
      <th>Dm</th>
      <th>Dp</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @caves.each do |cafe| %>
      <tr>
        <td><%= cafe.name %></td>
        <td><%= cafe.address %></td>
        <td><%= cafe.lat %></td>
        <td><%= cafe.lng %></td>
        <td><%= cafe.LM %></td>
        <td><%= cafe.LP %></td>
        <td><%= cafe.MM %></td>
        <td><%= cafe.MP %></td>
        <td><%= cafe.MEM %></td>
        <td><%= cafe.MEP %></td>
        <td><%= cafe.GM %></td>
        <td><%= cafe.GP %></td>
        <td><%= cafe.VM %></td>
        <td><%= cafe.VP %></td>
        <td><%= cafe.SM %></td>
        <td><%= cafe.SP %></td>
        <td><%= cafe.DM %></td>
        <td><%= cafe.DP %></td>
        <td><%= link_to 'Show', cafe %></td>
        <td><%= link_to 'Edit', edit_cafe_path(cafe) %></td>
        <td><%= link_to 'Destroy', cafe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cafe', new_cafe_path %>
